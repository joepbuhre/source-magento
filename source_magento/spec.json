{
    "documentationUrl": "https://docsurl.com",
    "connectionSpecification": {
        "$schema": "http://json-schema.org/draft-07/schema#",
        "title": "Magento Spec",
        "type": "object",
        "required": ["start_date", "magento_bearer"],
        "additionalProperties": false,
        "properties": {
          "start_date": {
            "type": "string",
            "description": "Start getting data from that date.",
            "examples": ["2022-01-01 00:00:00"]
          },
          "magento_bearer": {
            "type": "string",
            "description": "ISO reference currency. See <a href=\"https://www.ecb.europa.eu/stats/policy_and_exchange_rates/euro_reference_exchange_rates/html/index.en.html\">here</a>."
          },
          "page_size": {
            "type": "string",
            "description": "How many items you want to get with the same api call"
          }
        }
    }
}